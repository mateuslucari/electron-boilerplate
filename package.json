{
	"name": "electron-boilerplate",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/app/init.js",
	"homepage": "./",
	"scripts": {
		"clear": "npm run clear:dist && npm run clear:rel",
		"clear:dist": "rimraf ./dist",
		"clear:rel": "rimraf ./release",
		"start:dev": "electron .",
		"react:dev": "cross-env ENVIRONMENT=development TARGET=react webpack server",
		"elect:dev": "cross-env ENVIRONMENT=development TARGET=electron webpack --watch",
		"react:build": "cross-env ENVIRONMENT=production TARGET=react webpack",
		"elect:build": "cross-env ENVIRONMENT=production TARGET=electron webpack",
		"app:dev": "concurrently \"npm:react:dev\" \"npm:elect:dev\"",
		"app:dev:build": "npm run clear:dist && concurrently \"npm:react:build\" \"npm:elect:build\""
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.24.1",
		"@babel/preset-env": "^7.24.3",
		"@babel/preset-react": "^7.24.1",
		"@babel/preset-typescript": "^7.24.1",
		"@types/html-webpack-plugin": "^3.2.9",
		"@types/react": "^18.2.69",
		"@types/react-dom": "^18.2.22",
		"babel-loader": "^9.1.3",
		"babel-preset-env": "^1.7.0",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"electron": "^29.1.5",
		"html-loader": "^5.0.0",
		"html-webpack-plugin": "^5.6.0",
		"mini-css-extract-plugin": "^2.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.3",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.0.4",
		"webpack-electron-reload": "^1.0.1",
		"webpack-merge": "^5.10.0"
	}
}
